#include <iostream>
#include <fstream>
#include <string>
using namespace std;
int main()
{
    double volume, radius, bouancy, weight;
   cout<<"Enter in the  radius"<<endl;
   cin >> radius;
   volume = ((4 * 3.14*radius*radius*radius));
   bouancy = (volume * 62.4)/3;
   cout << " Enter in the weight"<<endl;
   cin>> weight;
   if(bouancy>=weight)
   {
       cout << "The bouancy is " << bouancy<<endl;
       cout << "the object will float"<< endl;
       
   }
  if(bouancy <=weight)
  {
      cout << "The weight is more than the " << bouancy<< endl;
      
  }
   
   

    return 0;
}
